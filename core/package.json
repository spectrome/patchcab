{
  "name": "@patchcab/core",
  "version": "1.1.3",
  "description": "Modular Eurorack style synthesizer made with Web Audio",
  "license": "MIT",
  "author": "Spectrome <rihards@gravis.lv>",
  "files": [
    "bin",
    "lib",
    "public"
  ],
  "main": "lib/lib.js",
  "types": "lib/lib.d.ts",
  "bin": {
    "patchcab": "./bin/bin.js"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "test": "svelte-check && tsc --noEmit",
    "build": "yarn build:lib && yarn build:web && yarn build:bin",
    "build:web": "rollup -c",
    "build:lib": "tsc --project ./config/lib.tsconfig.json && cpy './src/components/*.svelte' './lib/components/'",
    "build:bin": "tsc --project ./config/bin.tsconfig.json",
    "build:doc": "ts-node -P ./config/types.tsconfig.json ./config/svelteTypes.ts && typedoc ./types/lib.d.ts --tsconfig ./config/docs.tsconfig.json --out ./docs/",
    "lint": "eslint './src/**/*.ts' --max-warnings 0",
    "prettier": "prettier --write './src/**/*.{ts,svelte}'",
    "prepublish": "yarn build",
    "cli": "patchcab"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "dotenv": "^10.0.0",
    "file-dialog": "^0.0.8",
    "file-saver": "^2.0.5",
    "html-minifier-terser": "^6.0.2",
    "imagemin": "^7.0.1",
    "imagemin-pngquant": "^9.0.2",
    "ncp": "^2.0.0",
    "parse-author": "^2.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.5.0",
    "puppeteer": "^11.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.0",
    "rollup-plugin-external-globals": "^0.6.1",
    "rollup-plugin-glslify": "^1.2.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.0",
    "standardized-audio-context": "^25.3.15",
    "svelte": "^3.44.2",
    "svelte-check": "^2.2.10",
    "svelte-preprocess": "^4.9.8",
    "tone": "14.7.77",
    "typescript": "^4.1.3"
  },
  "prettier": {
    "useTabs": false,
    "arrowParens": "always",
    "semi": true,
    "bracketSpacing": true,
    "singleQuote": true,
    "printWidth": 120,
    "svelteSortOrder": "options-scripts-styles-markup"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "cpy-cli": "^3.1.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "svelte2tsx": "^0.4.10",
    "typedoc": "^0.22.10"
  }
}
